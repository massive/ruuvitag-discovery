{
    "name": "RuuviTag Discovery",
    "version": "0.2.4",
    "slug": "ruuvitag-discovery",
    "description": "Web interface to Discover RuuviTag Environmental Sensors and save measures to Home Assistant, MQTT, InfluxDB and Graphite",
    "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
    "url": "https://github.com/balda/ruuvitag-discovery",
    "startup": "application",
    "boot": "auto",
    "privileged": [
        "SYS_RAWIO"
    ],
    "hassio_api": true,
    "homeassistant_api": true,
    "host_network": true,
    "ingress": true,
    "ingress_port": 8099,
    "panel_icon": "mdi:access-point",
    "options": {
        "hci_device_id": 1
    },
    "schema": {
        "hci_device_id": "int"
    }
}
